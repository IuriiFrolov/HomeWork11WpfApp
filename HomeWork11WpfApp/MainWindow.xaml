<Window x:Class="HomeWork11WpfApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HomeWork11WpfApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="300">
    <Grid>
        <Path Fill="Red" Stroke="Black">
            <Path.Data>
                <PathGeometry>
                    <PathFigure StartPoint="190,100" IsClosed="True">
                        <ArcSegment Point="100,190" Size="64,64" SweepDirection="Counterclockwise"/>
                    </PathFigure>
                    <PathFigure StartPoint="200,110" IsClosed="True">
                        <ArcSegment Point="110,200" Size="64,64" SweepDirection="Clockwise"/>
                    </PathFigure>
                    <PathFigure StartPoint="70,150" >
                        <ArcSegment Point="230,150" Size="80,80" SweepDirection="Counterclockwise"/>
                    </PathFigure>
                    <PathFigure StartPoint="70,150" >
                        <ArcSegment Point="230,150" Size="80,80" SweepDirection="Clockwise"/>
                    </PathFigure>
                </PathGeometry>
            </Path.Data>
        </Path>
    </Grid>
</Window>
